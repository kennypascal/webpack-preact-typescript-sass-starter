{
  "name": "webpack-preact-typescript-sass-starter",
  "title": "Webpack Preact Typescript SASS",
  "version": "0.1.0",
  "author": "",
  "private": true,
  "description": "",
  "main": "",
  "license": "MIT",
  "config": {
    "port": "7799"
  },
  "scripts": {
    "test": "jest",
    "setup": "npm install && node tools/messages/setup.js && node tools/setup.js",
    "start": "npm run ip && webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --config webpack.config.dev.js",
    "build": "webpack -p --progress --colors --config webpack.config.build.js && node tools/messages/build.js",
    "build:analyze": "webpack -p --progress --colors --config webpack.config.build.js --env.analyze && node tools/messages/build.js",
    "build:inline-source": "webpack -p --progress --colors --config webpack.config.build.js --env.inline-source && node tools/messages/build.js",
    "ip": "node tools/messages/ip.js",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css,scss,js,jsx,json}\""
  },
  "dependencies": {
    "classnames": "^2.2.6",
