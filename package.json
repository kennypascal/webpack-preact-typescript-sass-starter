{
  "name": "webpack-preact-typescript-sass-starter",
  "title": "Webpack Preact Typescript SASS",
  "version": "0.1.0",
  "author": "",
  "private": true,
  "description": "",
  "main": "",
  "license": "MIT",
  "config": {
    "port": "7700"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "npm install && npm run message:setup && node tools/setup.js",
    "start": "npm run message:ip && webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --config webpack.config.dev.js",
    "build": "webpack -p --progress --colors --config webpack.config.build.js && npm run build:complete --silent",
    "build:analyze": "webpack -p --progress --colors --config webpack.config.build.js --env.analyze && npm run build:complete --silent",
    "build:complete": "npm run prettier:html && npm run message:build-complete && npm run message:build-server",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css,scss,js,jsx,json}\"",
    "prettier:html": "prettier --write \"build/**/*.html\"",
    "message:setup": "node tools/messages/setup.js",
    "message:build-complete": "node tools/messages/build.js",
    "message:build-server": "node tools/server.js",
    "message:ip": "node tools/messages/ip.js"
  },
  "dependencies": {
    "@types/react": "^16.9.35",
    "classnames": "^2.2.6",
    "preact": "^10.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^14.0.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "breakpoint-sass": "^2.7.1",
    "chalk": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "ejs-loader": "^0.3.6",
    "fibers": "^5.0.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^4.3.0",
    "ip": "^1.1.5",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.31",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-custom-properties": "^9.1.1",
    "postcss-discard-comments": "^4.0.2",
    "postcss-flexibility": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "postcss-sass-loader": "^1.1.0",
    "postcss-scss": "^2.1.1",
    "postcss-url": "^8.0.0",
    "prettier": "^2.0.5",
    "prompt": "^1.0.0",
    "raw-loader": "^4.0.1",
    "react-hot-loader": "4.12.21",
    "replace": "^1.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
