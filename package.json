{
  "name": "webpack-preact-typescript-sass-starter",
  "title":"Webpack Preact Typescript SASS",
  "version": "0.1.0",
  "author": "",
  "private": true,
  "description": "",
  "main": "",
  "license": "MIT",
  "config": {
    "port": "7799"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "npm install && node tools/messages/setup.js && node tools/setup.js",
    "start": "npm run ip && webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --config webpack.config.dev.js",
    "build": "webpack -p --progress --colors --config webpack.config.build.js && node tools/messages/build.js",
    "build:analyze": "webpack -p --progress --colors --config webpack.config.build.js --env.analyze && node tools/messages/build.js",
    "build:inline-source": "webpack -p --progress --colors --config webpack.config.build.js --env.inline-source && node tools/messages/build.js",
    "ip": "node tools/messages/ip.js",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "preact": "^8.4.2",
    "preact-compat": "^3.18.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@types/node": "^11.11.3",
    "breakpoint-sass": "^2.7.1",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "ejs-loader": "^0.3.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.5",
    "make-runnable": "^1.3.6",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.14",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-custom-properties": "^8.0.9",
    "postcss-discard-comments": "^4.0.2",
    "postcss-flexibility": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.16.4",
    "prompt": "^1.0.0",
    "raw-loader": "^1.0.0",
    "react-hot-loader": "4.7.1",
    "replace": "^1.1.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
